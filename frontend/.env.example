# Environment: local | testnet | production
# testnet = Arc Testnet (chain ID 5042002)
# production = Arc Mainnet (chain ID 1243, placeholder)
NEXT_PUBLIC_ENV=testnet

# WalletConnect Project ID (get from https://cloud.walletconnect.com)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=

# Contract addresses (Arc Testnet defaults)
NEXT_PUBLIC_USDC_ADDRESS=0x3600000000000000000000000000000000000000
NEXT_PUBLIC_LIQUIDITY_POOL_ADDRESS=0xb67db96eebf1d30d95a382535afbb2375ecf0219
NEXT_PUBLIC_INVOICE_DIAMOND_ADDRESS=0xe73911fbe91a0f76b20b53a8bd7d4c84c5532da6
NEXT_PUBLIC_EXECUTION_POOL_ADDRESS=0xf1389407cb618990b838d549c226de9ec2d447f0
# NEXT_PUBLIC_TREASURY_MANAGER_ADDRESS=

# Custom RPC (optional, default: https://rpc.testnet.arc.network)
# NEXT_PUBLIC_RPC_URL=

# Database connection
# Local: postgresql://postgres:postgres@localhost:5432/seed_finance
# Railway: ${{Postgres.DATABASE_URL}} (set in Railway dashboard)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/seed_finance
